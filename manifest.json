{
"manifest_version": 2,

"name": "rrss",
"description": "R is a reader of RSS feeds",
"version": "0.5.3",

"oauth2":
{
  "client_id": "1043072112628-tbg2533tg0vts1ihq5c9m12m2b9kk9cs.apps.googleusercontent.com",
  "scopes":
  [
    "https://www.googleapis.com/auth/drive"
  ]
},

"key": "",

"icons":
{
  "128": "chrome/icon_rss.png"
},

"permissions":
[
  "identity",
  "identity.email",
  "storage",
  "https://ssl.gstatic.com/",
  "https://www.googleapis.com/",
  "https://www.google.com/",
  "https://accounts.google.com/",
  "http://*/",
  "https://*/"
],

"content_security_policy": "script-src 'self' https://apis.google.com https://drive.google.com; object-src 'self'",

"browser_action" :
{
  "default_title": "rrss (devel)",
  "default_icon" : "chrome/icon_rss.png"
},

"background":
{
   "persistent": false,
   "scripts": [ "chrome/background.js" ]
}

}
